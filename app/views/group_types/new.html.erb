<% content_for :title, title('Create a group type', 'Group Types') %>

<% content_for :breadcrumbs,
	breadcrumbs(['Group Types',group_types_url]) %>
<% content_for :subheader_title, 'Create a new group type' %>

<% content_for :body do %>
	<%= error_messages_for :group_type %>
	<% form_for @group_type do |f| -%>
		<%= f.label :name %>
		<%= f.text_field :name %>
		<p><%= f.submit 'Create' %></p>
	<% end %>
<% end %>
